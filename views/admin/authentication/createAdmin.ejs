<!-- user adding form -->

<div class="container px-4">
  <form action="/adminDahboard/createAdmin" method="post">
      <div class="row">
          <div class="form-group py-2 col">
              <label for="fname">First Name:</label>
              <input type="text" class="form-control" id="fname" name="fname" placeholder="First Name" required>
          </div>
          <div class="form-group py-2 col">
              <label for="lname">Last Name:</label>
              <input type="text" class="form-control" id="lname" name="lname" placeholder="Last Name" required>
          </div>
      </div>

      <div class="row">
          <div class="form-group py-2 col">
              <label for="email">Email:</label>
              <input type="email" class="form-control" id="email" name="email" placeholder="example@gmail.com"
                  required>
          </div>
          <div class="form-group py-2 col">
              <label for="tel">phone No:</label>
              <input type="tel" class="form-control" id="tel" name="tel" placeholder="+91" required>
          </div>
      </div>

      <div class="row">
        <div class="form-group py-2 col">
            <label for="uname">User Name:</label>
            <input type="text" class="form-control" id="uname" name="uname" placeholder="Set User Name" required>
        </div>
        <div class="form-group py-2 col">
            <label for="pwd">Password:</label>
            <input type="password" class="form-control" id="pwd" name="password" placeholder="Set Password" required>
        </div>
         
      </div>

      <button class="btn btn-primary mt-4" type="submit"><i class="bi bi-person-fill-add"></i> Add User</button>
  </form>
</div>